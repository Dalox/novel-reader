{
  "extends": ["tslint:recommended", "tslint-react", "tslint-config-prettier"],
  "rulesDirectory": ["tslint-plugin-prettier"],
  "rules": {
    "prettier": true,
    "interface-name": false,
    "object-literal-sort-keys": false,
    "jsx-no-lambda": false
  },

  "linterOptions": {
    "exclude": ["node_modules","build", "src/serviceWorker.ts"]
  }
}
